{
    "compilerOptions": {
      "lib": ["ESNext"],
      "module": "esnext",
      "target": "esnext",
      "moduleResolution": "node",
      "strict": true,
      "paths": {
        "~/*": ["./src/*"]
      },
      "types": ["bun-types"],
       "outDir": "dist/",
    },
    "exclude": [
        "node_modules",
        "dist/",
        "deno_dist/",
        "src/**/*.deno.ts",
        "src/**/*.deno.tsx",
   ]
}
