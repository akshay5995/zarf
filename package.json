{
  "name": "bun-tea",
  "version": "0.0.1-alpha.8",
  "description": "Fast, Bun-powered, and Bun-only(for now) Web API framework with full Typescript support.",
  "homepage": "https://github.com/one-aalam/bun-tea",
  "repository": {
    "type": "git",
    "url": "https://github.com/one-aalam/bun-tea.git"
  },
  "bugs": "https://github.com/one-aalam/bun-tea/issues",
  "keywords": [
    "bun",
    "web",
    "framework",
    "http",
    "middleware"
  ],
  "author": "Aftab Alam <one.aalam@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon --config nodemon.json",
    "build": "bun run build:tsup --dts-resolve",
    "build:tsup": "tsup",
    "release": "bumpp --commit --push --tag && npm publish",
    "prepublishOnly": "bun run build",
    "test": "bun wiptest"
  },
  "dependencies": {
    "es-mime-types": "^0.1.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "bumpp": "^8.2.1",
    "bun-types": "^0.1.8",
    "eslint": "^8.22.0",
    "nodemon": "^2.0.19",
    "tsup": "^6.2.2",
    "typescript": "^4.7.4"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./utils/parsers/qs": {
      "types": "./dist/utils/parsers/qs.d.ts",
      "require": "./dist/utils/parsers/qs.js",
      "import": "./dist/utils/parsers/qs.mjs"
    },
    "./utils/parsers/json": {
      "types": "./dist/utils/parsers/json.d.ts",
      "require": "./dist/utils/parsers/json.js",
      "import": "./dist/utils/parsers/json.mjs"
    },
    "./utils/parsers/body": {
      "types": "./dist/utils/parsers/body.d.ts",
      "require": "./dist/utils/parsers/body.js",
      "import": "./dist/utils/parsers/body.mjs"
    },
    "./utils/is": {
      "types": "./dist/utils/is.d.ts",
      "require": "./dist/utils/is.js",
      "import": "./dist/utils/is.mjs"
    },
    "./utils/mime": {
      "types": "./dist/utils/mime.d.ts",
      "require": "./dist/utils/mime.js",
      "import": "./dist/utils/mime.mjs"
    },
    "./utils/sanitize": {
      "types": "./dist/utils/sanitize.d.ts",
      "require": "./dist/utils/sanitize.js",
      "import": "./dist/utils/sanitize.mjs"
    }
  },
  "files": [
    "dist"
  ]
}
